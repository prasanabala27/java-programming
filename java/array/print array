method-1
public class Main
{
	public static void main(String[] args) {
	    int[] a = {1,2,3,4,5};
	    for (int i=0;i<a.length;i++){ 
		System.out.println(a[i]);
	    }
	}
}

method-2
public class Main
{
	public static void main(String[] args) {
	    int[] a = {1,2,3,4,5};
	    for (int i:a){ 
		System.out.println(i);
	    }
	}
}
